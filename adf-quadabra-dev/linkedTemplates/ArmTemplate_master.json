{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-quadabra-dev"
		},
		"key_quadabra_bigdata_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-bigdataqu-dev.vault.azure.net/"
		},
		"cnn_datalake_quadabra_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalaketaz.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-quadabra-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"key_quadabra_bigdata_properties_typeProperties_baseUrl": {
						"value": "[parameters('key_quadabra_bigdata_properties_typeProperties_baseUrl')]"
					},
					"cnn_datalake_quadabra_properties_typeProperties_url": {
						"value": "[parameters('cnn_datalake_quadabra_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}